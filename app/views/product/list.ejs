<html>
<body>
<h1>Produtos</h1>
<table>
    <tr>
        <th>Id</th>
        <th>SKU</th>
        <th>Title</th>
        <th>Price</th>
        <th></th>
    </tr>
    <% for (i in products) { %>
    <tr>
        <td><%= products[i].id %></td>
        <td><%= products[i].sku %></td>
        <td><%= products[i].title %></td>
        <td><%= products[i].price %></td>
        <td><a href="/products/form?id=<%=products[i].id%>">Edit</a></td>
    </tr>
    <% } %>
</table>
</body>
</html>